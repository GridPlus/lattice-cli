{
  "name": "gridplus-cli",
  "version": "0.0.1",
  "description": "CLI for interacting with gridplus-sdk",
  "main": "./dist/gridplus-cli.js",
  "types": "./dist/gridplus-cli.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "ts-node ./src/gridplus-cli.ts",
    "release": "npm run build && npx pkg -c package.json dist/gridplus-cli.js"
  },
  "keywords": [],
  "author": "Doug Lance",
  "license": "ISC",
  "dependencies": {
    "cli-progress": "^3.11.2",
    "dotenv": "^16.0.0",
    "enquirer": "^2.3.6",
    "gridplus-sdk": "^2.4.1",
    "lattice-eth2-utils": "^0.2.0",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "typescript": "^4.6.3"
  },
  "pkg": {
    "targets": [ "node18-linux-x64", "node18-macos-x64", "node18-win-x64" ],
    "outputPath": "release"
  }
}
